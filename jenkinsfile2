pipeline
{
    agent any
    stages
    {
        stage('ContinuousDownload_Loan')
        {
            steps
            {
              git 'https://github.com/jishikasaranyaa/maven3.git' 
            }
        }
        stage('ContinuousBuild_Loans')
        {
            steps
            {
                sh 'mvn package'
            }
        }
        
    }
}
